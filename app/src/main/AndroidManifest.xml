<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.grupocaravela.comprefacil.velejaratacado">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.FLAG_GRANT_READ_URI_PERMISSION" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.CompreFacil"
        android:screenOrientation="portrait">
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.ConfiguracaoActivity"
            android:label="@string/title_activity_configuracao"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.AjudaActivity"
            android:label="@string/title_activity_ajuda"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.ProdutosActivity"
            android:label="@string/title_activity_produtos"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.CadastroClienteActivity"
            android:label="@string/title_activity_cadastro_cliente"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.ErroServidorActivity"
            android:label="@string/title_activity_erro_servidor"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.CarrinhosComprasCabecalhoActivity"
            android:label="@string/title_activity_carrinhos_compras_cabecalho"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.CaixaActivity"
            android:label="Caixa"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.LoginActivity"
            android:label="Velejar ATACADO MBILE"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.ClientesActivity"
            android:label="@string/title_activity_clientes"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.VendaActivity"
            android:label="@string/title_activity_venda"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.DetalhesClienteActivity"
            android:label="@string/title_activity_detalhe_cliente"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.AddClientesActivity"
            android:label="@string/title_activity_clientes"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.AddProdutosActivity"
            android:label="@string/title_activity_produtos"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.DetalhesProdutoActivity"
            android:label="@string/title_activity_detalhe_cliente"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.FinalizarVendaActivity"
            android:label="@string/title_activity_finalizar_venda"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.ContasReceberActivity"
            android:label="@string/title_activity_contas_receber"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.DetalhesContaReceberActivity"
            android:label="@string/title_activity_detalhes_conta_receber"
            android:screenOrientation="portrait"/>
        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.AddFormaPagamentoActivity"
            android:label="@string/title_activity_add_forma_pagamento"
            android:screenOrientation="portrait" />

        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.MenuPrincipalActivity"
            android:label="@string/title_activity_menu_principal"
            android:screenOrientation="portrait" />

        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.AtualizarActivity"
            android:label="@string/title_activity_atualizar"
            android:screenOrientation="portrait" />

        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.HistoricoVendasActivity"
            android:label="@string/title_activity_historico_vendas"
            android:screenOrientation="portrait" />

        <activity
            android:name="br.com.grupocaravela.velejar.atacadomobile.Util.PermissionsActivity"
            android:label="@string/title_activity_atualizar"
            android:screenOrientation="portrait" />

        <!-- File Provider -->
        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="br.com.grupocaravela.velejar.atacado.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/provider_paths" />
        </provider>

        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>
